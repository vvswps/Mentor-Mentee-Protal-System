<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
    pageEncoding="ISO-8859-1"%>
<!DOCTYPE html>
<html>
<head>
	<title>Technical Support</title>
	<style>
		body {
			font-family: Arial, sans-serif;
			background-size:cover;
			background-repeat: no-repeat;
			background-image: url("technicalsupport.jpg");
		}

		.container {
			
			margin-top: 280px;
			margin-left: 450px;
			padding: 30px;
			background-color: #fff;
			box-shadow: 0 0 10px rgba(0,0,0,0.2);
			max-width: 600px;
		}

		h1 {
			font-size: 36px;
			margin-top: 0;
			text-align: center;
			color: #4169e1;
		}

		.contact-info {
			display: flex;
			flex-wrap: wrap;
			align-items: center;
			justify-content: center;
			margin-bottom: 30px;
		}

		.contact-info div {
			margin: 10px;
		}

		.contact-info a {
			display: block;
			font-size: 18px;
			font-weight: bold;
			color: #080808;
			text-decoration: none;
			transition: all 0.3s ease-in-out;
		}

		.contact-info a:hover {
			color: #0056b3;
			text-decoration: underline;
		}

		.contact-info img {
			margin-right: 10px;
			vertical-align: middle;
		}

	</style>
</head>
<body>
	<div class="container">
		<h1>Technical Support</h1>
		<div class="contact-info">
			<div>
				
					<b>Call us @ 6203421651</b>
					
				
			</div>
			<div>
				<a href="mailto: iamtatva@gmail.com">
					Email us @ iamtatva@gmail.com
				</a>
			</div>
		</div>

	</div>
</body>
</html>
